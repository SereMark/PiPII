{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch Streamlit Dashboard",
        "type": "debugpy",
        "request": "launch",
        "module": "streamlit",
        "console": "integratedTerminal",
        "env": {
          "PYTHONPATH": "${workspaceFolder}"
        },
        "args": ["run", "src/dashboard.py"]
      }
    ]
  }  